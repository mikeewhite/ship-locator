scalar Date

type ShipSearchResult {
    MMSI: Int!
    Name: String!
}

type Ship {
    MMSI: Int!
    Name: String!
    Latitude: Float!
    Longitude: Float!
    LastUpdated: Date!
}

type Query {
    lookupShipByNameOrMMSI(searchTerm: String!): [ShipSearchResult]
    getShipByMMSI(mmsi: Int!): Ship
}